<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
	xmlns:core="http://www.milyn.org/xsd/smooks/smooks-core-1.4.xsd"
	xmlns:csv="http://www.milyn.org/xsd/smooks/csv-1.2.xsd">

	<!--
		Smooks v1.2 makes binding of CSV records very easy. Here
		we create a List of Customer objects...
	-->
	<core:exports>
		<core:result type="org.milyn.payload.JavaResult"/>
	</core:exports>

	<csv:reader fields="articleId,reserved,left">
		<csv:listBinding beanId="replyList" class="com.redhat.brq.integration.switchyard.models.InventoryReply"/>
	</csv:reader>

</smooks-resource-list>